
# Secret key can be generated by running either commands:
# python3 -c 'import secrets; print(secrets.token_urlsafe(32))'
# openssl rand -base64 32
SECRET_KEY=some_random_key

# Optional if you don't want AI summarization.
OPENAI_API_KEY=your_open_ai_api_key

# Change the `DEPLOYMENT_URL` to `hostname.yourdomain.tld` if you deploy with your domain.
# Do not include `http://` or `https://`
DEPLOYMENT_URL=hostname.yourdomain.tld

# Uncomment the following for customed OPENAI-compatible API, e.g. Using OneAPI
# If you are using OpenAI API only, leave it as it is.
#OPENAI_PROXY=
#OPENAI_BASE_URL='https://api.openai.com/v1'
